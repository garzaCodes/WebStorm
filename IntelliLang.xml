<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JSX Harmony" injector-id="xml">
      <display-name>a/@target</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("target")).withParent(xmlTag().withLocalName(string().equalTo("a")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>